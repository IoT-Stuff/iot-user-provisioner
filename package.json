{
  "name": "ts_backend_boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "DEBUG=iot-user-provisioner* dotenv -e .env nodemon",
    "start": "dotenv -e .env npm run build:prod; dotenv -e .env node ./build/index.js",
    "build:prod": "rimraf ./build; tsc",
    "tests:unit": "npx mocha --recursive -r ts-node/register ./test/**/*.unit.ts",
    "tests:e2econfig": "cucumber-js -p default",
    "tests:e2e": "dotenv -e .env ./scripts/e2e.test.sh",
    "tests:_e2e": "npx dotenv -e .env cucumber-js -- test/e2e/features/**/*.feature --require-module ts-node/register --require ./test/e2e/steps/**/*.steps.ts",
    "tests:e2eonly": "cucumber-js -p default --name '@only'",
    "tests:coverage": "dotenv -e .env nyc npm run tests:unit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.5",
    "@types/cors": "^2.8.6",
    "@types/cucumber": "^6.0.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.5.32",
    "@types/node": "^12.12.9",
    "@types/superagent": "^4.1.4",
    "@types/winston": "^2.4.4",
    "chai": "^4.2.0",
    "cucumber": "^6.0.5",
    "cucumber-pretty": "^6.0.0",
    "cucumber-tsflow": "^3.2.0",
    "dotenv-cli": "^3.1.0",
    "mocha": "^6.2.2",
    "mongo-unit": "^1.4.5",
    "nodemon": "^1.19.4",
    "rimraf": "^3.0.0",
    "superagent": "^5.1.1",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "http-status-codes": "^1.4.0",
    "mongoose": "^5.7.13",
    "winston": "^3.2.1"
  }
}
