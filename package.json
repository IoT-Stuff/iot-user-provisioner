{
  "name": "iot-user-provisioner",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "eslint:run": "eslint src",
    "eslint:fix": "eslint './src/**/*.{ts,tsx}' --fix",
    "start:dev": "DEBUG=iot-user-provisioner* dotenv -e .env nodemon",
    "start": "dotenv -e .env npm run build:prod; dotenv -e .env node ./build/index.js",
    "build:prod": "npx rimraf ./node_modules ./build; npm i; tsc",
    "tests:unit": "npx mocha --recursive -r ts-node/register ./test/**/*.unit.ts",
    "tests:e2econfig": "cucumber-js -p default",
    "tests:e2e_container": "dotenv -e .env ./scripts/e2e.test.sh",
    "tests:e2e": "npx dotenv -e .env cucumber-js -- test/e2e/features/**/*.feature --require-module ts-node/register --require ./test/e2e/steps/**/*.steps.ts",
    "tests:e2eonly": "npx dotenv -e .env cucumber-js -- test/e2e/features/**/*.feature --require-module ts-node/register --require ./test/e2e/steps/**/*.steps.ts  --name '@only'",
    "tests:coverage": "dotenv -e .env nyc --reporter=html npm run tests:unit"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.5",
    "@types/cors": "^2.8.6",
    "@types/cucumber": "^6.0.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/mocha": "^5.2.7",
    "@types/mongoose": "^5.5.32",
    "@types/node": "^12.12.9",
    "@types/superagent": "^4.1.4",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "chai": "^4.2.0",
    "cucumber": "^6.0.5",
    "cucumber-pretty": "^6.0.0",
    "cucumber-tsflow": "^3.2.0",
    "dotenv-cli": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "mocha": "^6.2.2",
    "mongo-unit": "^1.4.5",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "superagent": "^5.1.1",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@iot-stuff/iot-logger": "^1.1.0",
    "ajv": "^6.10.2",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "http-status-codes": "^1.4.0",
    "mongoose": "^5.7.13"
  }
}
